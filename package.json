{
  "name": "phaser3-react-template",
  "version": "1.0.2",
  "description": "Phaser 3 starter template with TypeScript , webpack, react, reduxjs and websocket.",
  "homepage": "https://github.com/tfkfan/phaser3-react-template#readme",
  "author": {
    "name": "tfkfan",
    "url": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tfkfan/phaser3-react-template.git"
  },
  "template": {
    "name": "phaser3-react-template",
    "url": "https://github.com/tfkfan/phaser3-react-template",
    "author": "TFKFAN (https://github.com/tfkfan)"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "npm run webapp:prod --",
    "clean-www": "rimraf build/resources/main/static/app/{src,build/}",
    "cleanup": "rimraf build/resources/main/static/",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --check \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\"",
    "start": "npm run webapp:dev --",
    "start-tls": "npm run webapp:dev -- --env.tls",
    "pretest": "npm run lint",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev --",
    "webapp:build:dev": "webpack --config webpack/webpack.dev.js --env stats=minimal",
    "webapp:build:prod": "webpack --config webpack/webpack.prod.js --progress=profile",
    "webapp:dev": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --env stats=minimal",
    "webapp:dev-verbose": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --progress=profile --env stats=normal",
    "webapp:prod": "npm run cleanup && npm run webapp:build:prod --",
    "webpack-dev-server": "webpack serve"
  },
  "config": {
    "backend_port": "8080",
    "default_environment": "prod",
    "packaging": "jar"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@reduxjs/toolkit": "1.8.1",
    "axios": "1.8.2",
    "bootstrap": "5.1.3",
    "bootswatch": "5.1.3",
    "browser-sync-client": "^2.27.9",
    "path-browserify": "1.0.1",
    "phaser": "^3.88.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.8",
    "react-router-dom": "5.3.0",
    "react-toastify": "8.2.0",
    "react-transition-group": "4.4.2",
    "reactstrap": "^8.10.1",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1"
  },
  "devDependencies": {
    "@types/lodash": "4.14.181",
    "@types/node": "16.11.26",
    "@types/react": "^18.2.6",
    "@types/react-dom": "17.0.14",
    "@types/react-redux": "7.1.23",
    "@types/react-router-dom": "5.3.3",
    "@types/redux": "3.6.31",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "autoprefixer": "10.4.4",
    "browser-sync": "^2.27.9",
    "browser-sync-webpack-plugin": "2.3.0",
    "copy-webpack-plugin": "10.2.4",
    "core-js": "3.21.1",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "3.4.1",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-webpack-plugin": "3.1.1",
    "filemanager-webpack-plugin": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.3",
    "html-webpack-plugin": "5.5.0",
    "identity-obj-proxy": "3.0.0",
    "json-loader": "0.5.7",
    "lint-staged": "12.3.7",
    "mini-css-extract-plugin": "2.6.0",
    "postcss-loader": "6.2.1",
    "prettier": "2.6.2",
    "prettier-plugin-java": "1.6.1",
    "prettier-plugin-packagejson": "2.2.17",
    "react-infinite-scroll-component": "6.1.0",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.2",
    "sass": "1.49.11",
    "sass-loader": "12.6.0",
    "simple-progress-webpack-plugin": "2.0.0",
    "sinon": "13.0.1",
    "source-map-loader": "3.0.1",
    "sourcemap-istanbul-instrumenter-loader": "0.2.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "thread-loader": "3.0.4",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "webpack": "5.76.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.8.0",
    "webpack-merge": "5.8.0",
    "webpack-notifier": "1.15.0",
    "workbox-webpack-plugin": "6.5.2"
  }
}
